{
  "name": "donacoop",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:gateway": "npm --prefix gateway run start:dev",
    "build:gateway:prod": "npm --prefix gateway run install && npm --prefix gateway run build",
    "start:gateway:prod": "npm --prefix gateway run start:prod"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "20.1.0",
    "@angular/common": "20.1.0",
    "@angular/compiler": "20.1.0",
    "@angular/core": "20.1.0",
    "@angular/forms": "20.1.0",
    "@angular/material": "20.1.0",
    "@angular/platform-browser": "20.1.0",
    "@angular/router": "20.1.0",
    "@kolkov/angular-editor": "3.0.0-beta.2",
    "crypto-js": "4.2.0",
    "material-icons": "1.13.14",
    "ngx-mat-select-search": "8.0.2",
    "rxjs": "7.8.0",
    "tslib": "2.3.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular/animations": "20.1.0",
    "@angular/build": "20.1.1",
    "@angular/cli": "20.1.1",
    "@angular/compiler-cli": "20.1.0",
    "@types/crypto-js": "4.2.2",
    "@types/jasmine": "5.1.0",
    "jasmine-core": "5.8.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "typescript": "5.8.2"
  }
}
